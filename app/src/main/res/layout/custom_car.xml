<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="80dp"
    android:padding="16dp"
    android:background="@drawable/bg_white_underline_black">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cupo"
        android:layout_marginBottom="3dp"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/quantityTv" />
    <TextView
        android:id="@+id/quantityTv"

        android:text="Cupo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="64dp"
        android:layout_alignTop="@+id/finishEt"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_marginBottom="3dp"
        android:text="Desde"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2" />
    <TextView
        android:text="DESDE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/startEt"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/textView2"
        android:layout_marginStart="18dp" />

    <TextView
        android:text="Hasta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:id="@+id/finishEt"
        android:text="HASTA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/startEt" />

</RelativeLayout>
